<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>HistoryCleaner</AssemblyName>
    <RootNamespace>HistoryCleaner</RootNamespace>
    <ApplicationManifest>..\HistoryCleaner\app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\HistoryCleaner\**\*.cs" />
    <EmbeddedResource Include="..\HistoryCleaner\**\*.resx" />
    <None Include="..\HistoryCleaner\app.config" />
  </ItemGroup>

</Project>
